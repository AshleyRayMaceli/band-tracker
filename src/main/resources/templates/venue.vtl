<h1 class="venue-title">$venue.getName()</h1>

#if ($venue.getBands().size() != 0)
  <h2>Bands who have played at $venue.getName():</h2>
  #foreach ($band in $bands)
    <a href="/bands/$band.getId()">$band.getName()</a>
    <br>
  #end
#else
  <h2>No bands have played at $venue.getName() yet</h2>
#end

<br>
<a href="/" class="btn btn-info">Back To Home</a>
